{
  "name": "transparent-herd",
  "version": "1.2.2",
  "description": "A package for grouping singular calls into bulk or batch calls transparently to the caller",
  "scripts": {
    "build": "tsc",
    "doc": "typedoc",
    "test": "jest --coverage --config jestconfig.json",
    "lint": "eslint --fix src/**/*.ts test/**/*.ts",
    "format": "prettier --write ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emasab/transparent-herd.git"
  },
  "keywords": [
    "bulk",
    "batch",
    "transparent",
    "calls"
  ],
  "author": "Emanuele Sabellico",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.7",
    "typescript": "^4.1.2"
  }
}
