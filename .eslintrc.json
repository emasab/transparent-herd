{
    "extends": [
        "google",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "plugins": [
        "eslint-plugin-tsdoc"
    ],
    "rules": {
      "tsdoc/syntax": "error",
      "valid-jsdoc": "off"
    }
}